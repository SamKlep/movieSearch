<link href="/css/style.css" rel="stylesheet" type="text/css">
<%- include("partials/header") %>

<h1>
    Results Page
</h1>
<a href='/'>Search Again!</a>

<ul>
    <% data["Search"].forEach(function(movie){ %>
    <li>
        <strong>
            <%= movie["Title"] %>
        </strong>
        - <%= movie["Year"] %>
    </li>
    <li><%=  %></li>
    <li><img src='<%= movie["Poster"] %>' /></li>
    <% }) %>
</ul>

<%- include("partials/footer") %>